<script>
function _convert(e) {
var table = document.getElementById('gauge-table');
     for( var r = 1; r < table.rows.length; r++ ) {
     	  var gaugeNumber = table.rows[r].cells[0].innerHTML;
	  console.log( gaugeNumber );
	  // Strip the 'ga'
	  if( gaugeNumber == '00ga' ) gaugeNumber = -1;
	  else if( gaugeNumber == '000ga' ) gaugeNumber = -2;
	  else if( gaugeNumber == '0000ga' ) gaugeNumber = -3;
	  else if( gaugeNumber == '00000ga' ) gaugeNumber = -4;
	  else gaugeNumber = parseInt(gaugeNumber);
	  if( isNaN(gaugeNumber) )
	      continue;
	  // See
	  //    https://www.rapidtables.com/calc/wire/wire-gauge-chart.html
	  var inch = 0.005 * Math.pow( 92, (36-gaugeNumber)/39 );
	  var mill = 0.127 * Math.pow( 92, (36-gaugeNumber)/39 );
	  console.log( 'gauge=' + gaugeNumber + ', inch=' + inch + ', mill=' + mill );
	  table.rows[r].cells[1].innerHTML = mill.toFixed(4)+'mm';
	  table.rows[r].cells[2].innerHTML = inch.toFixed(5)+'in';
     }
} // END function

document.getElementById('convert').addEventListener('click', _convert );
window.addEventListener('load',_convert);

</script>



